{
  "name": "fp_study_notes_intro_to_react",
  "version": "1.0.0",
  "description": "[Intro to React](https://github.com/getfutureproof/fp_guides_wiki/wiki/Intro-to-React)",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-cli serve --mode development --config config/webpack.config.dev.js",
    "build": "webpack --config config/webpack.config.production.js",
    "test": "jest --watch --setupFilesAfterEnv ./src/test/setupTests.js",
    "coverage": "jest --setupFilesAfterEnv ./src/test/setupTests.js --coverage --watchAll=false"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfutureproof/fp_study_notes_intro_to_react.git"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.1",
    "css-loader": "^5.0.1",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^27.0.6",
    "style-loader": "^2.0.0",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.9"
  }
}
